{
    "manifest_version": 2,
    "name": "The Fucking Github",
    "version": "1.0.2",
    "description": "View starred repositories, organizing stars, searching stars for Github.",
    "homepage_url": "https://github.com/lvxianchao/the-fucking-github",
    "icons": {
        "128": "icons/128.png",
        "32": "icons/32.png",
        "64": "icons/64.png",
        "16": "icons/16.png",
        "48": "icons/48.png"
    },
    "browser_action": {
        "default_icon": "icons/48.png"
    },
    "permissions": [
        "storage",
        "unlimitedStorage",
        "activeTab",
        "tabs",
        "https://*.github.com/*",
        "https://*.githubusercontent.com/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    }
}